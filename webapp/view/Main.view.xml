<mvc:View
    controllerName="players.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m">
    <Page
        id="page"
        title="{i18n>title}">
        <List
            id="players"
            items="{/AttributesSet}">
            <StandardListItem
                description="{position}"
                title="{club}"
            />
        </List>
    </Page>
</mvc:View>
